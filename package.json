{
  "name": "gatsby-starter-hello-world",
  "description": "Gatsby hello world starter",
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "cp_redirects": "cp ./_redirects public/_redirects",
    "cp_headers": "cp ./_headers public/_headers",
    "cp_robots": "cp ./robots.txt public/robots.txt",
    "deploy": "gatsby build && npm run cp_redirects && npm run cp_headers && npm run cp_robots"
  },
  "dependencies": {
    "gatsby": "^2.0.0-beta.98",
    "gatsby-link": "^2.0.0-beta.20",
    "gatsby-plugin-sass": "^2.0.0-beta.10",
    "gatsby-source-filesystem": "^2.0.1-beta.10",
    "gatsby-transformer-remark": "^1.7.44",
    "node-sass": "^4.9.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
